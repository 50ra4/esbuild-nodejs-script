    "do:batch1": "npm run check-type && npm run build && node dist/batch1/index.js"
  },
  "devDependencies": {
    "@types/encoding-japanese": "2.0.5",
    "@types/node": "20.11.5",
    "esbuild": "0.19.11",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "axios": "1.6.5",
    "csv-parse": "5.5.3",
    "date-fns": "3.2.0",
    "encoding-japanese": "2.0.0"
  }
}
