{
  "name": "esbuild-nodejs-script",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf dist && node --loader ts-node/esm src/scripts/build.ts",
    "check-type": "tsc --noEmit",
    "lint:lint-staged": "lint-staged",
    "lint:prettier": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "lint:eslint": "eslint --fix 'src/**/*.{js,ts,tsx}'",
    "lint": "npm run lint:prettier && npm run lint:eslint",
    "do:batch1": "npm run check-type && npm run build && node dist/batch1/index.mjs"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.131",
    "@types/encoding-japanese": "2.0.5",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "husky": "8.0.0",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "axios": "1.6.5",
    "csv-parse": "5.5.3",
    "date-fns": "3.2.0",
    "encoding-japanese": "2.0.0"
  }
}
